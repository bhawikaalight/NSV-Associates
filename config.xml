<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sample.notify" 
        version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>NSV associates</name>
    <description>CA Rajneesh Verma</description>
    <author email="rajneeshverma10@gmail.com">rajneeshverma10@gmail.com</author>
    <content src="index.html" />
    <access origin="*" launch-external="yes"/>	
	<gap:plugin name="cordova-plugin-whitelist" spec="1.3.2" source="npm" />
    <plugin name="phonegap-plugin-push" spec="1.9.0" source="npm">
        <param name="SENDER_ID" value="713837838534" />
    </plugin>
    
    <allow-intent href="tel:*" />
	<allow-intent href="http:*" />
	<allow-intent href="https:*" />
    
	<preference name="stay-in-webview" value="false" />
	<preference name="orientation" value="portrait" />   
    <preference name="android-installLocation" value="auto" />
<icon src="files/logo.png" />	
</widget>